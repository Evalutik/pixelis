{
  "name": "Pixelis Dev Container",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "php",
  "workspaceFolder": "/var/www/html",
  "postCreateCommand": "composer install --no-interaction && php vendor/setup_db.php --seed=dev:dev",
  "forwardPorts": [8080],
  "extensions": ["bmewburn.vscode-intelephense-client", "bmewburn.vscode-intelephense-client"],
  "settings": {
    "php.validate.executablePath": "/usr/local/bin/php"
  }
}